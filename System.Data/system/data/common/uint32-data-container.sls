(library (system data common uint32-data-container)
  (export new is? uint32-data-container?)
  (import (ironscheme-clr-port))
  (define-syntax new
    (lambda (e)
      (syntax-case e ()
        ((_ a ...)
         #'(clr-new System.Data.Common.UInt32DataContainer a ...)))))
  (define (is? a) (clr-is System.Data.Common.UInt32DataContainer a))
  (define (uint32-data-container? a)
    (clr-is System.Data.Common.UInt32DataContainer a)))
