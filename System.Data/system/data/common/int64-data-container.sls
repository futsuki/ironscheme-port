(library (system data common int64-data-container)
  (export new is? int64-data-container?)
  (import (ironscheme-clr-port))
  (define-syntax new
    (lambda (e)
      (syntax-case e ()
        ((_ a ...)
         #'(clr-new System.Data.Common.Int64DataContainer a ...)))))
  (define (is? a) (clr-is System.Data.Common.Int64DataContainer a))
  (define (int64-data-container? a)
    (clr-is System.Data.Common.Int64DataContainer a)))
