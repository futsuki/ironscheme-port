(library (system data common uint64-data-container)
  (export new is? uint64-data-container?)
  (import (ironscheme-clr-port))
  (define-syntax new
    (lambda (e)
      (syntax-case e ()
        ((_ a ...)
         #'(clr-new System.Data.Common.UInt64DataContainer a ...)))))
  (define (is? a) (clr-is System.Data.Common.UInt64DataContainer a))
  (define (uint64-data-container? a)
    (clr-is System.Data.Common.UInt64DataContainer a)))
